<%= form_for @geekbuster do |f| %>
	<fieldset>
		<div class="clearfix">
			<%= f.label :first_name, :class=>"xlInput" %>
			<div class="input">
				<%= f.text_field :first_name, :class=>"xlInput" %>
			</div>
		</div>
		<div class="clearfix">
			<%= f.label :last_name, :class=>"xlInput" %>
			<div class="input">
				<%= f.text_field :last_name, :class=>"xlInput" %>
			</div>
		</div>	
		<div calss="clearfix">	
			<%= f.label :skills %>
			<div class="input">
				<%= f.select :skills, ["Rails", "MongoDB", "Java", "HTML", "CSS", "Project Management", "PHP", "Java"], {}, :class => "multiple", :multiple => true %>
			</div>
		</div>
		<div class="actions">
            <button type="submit" class="btn primary">Save Changes</button>
         </div>	
	</fieldset>
<% end %>